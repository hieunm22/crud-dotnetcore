@model IEnumerable<Billionaire>
<div class="paginationnew">
    <ul>
        <li class="inactive" title="First page" onclick="paging(this)" id="firstPage"><<</li>
        <li class="inactive" title="Previous page" onclick="paging(this)" id="prevPage"><</li>
        <span id="currentPageParent">
            <input id="currentPage" type="number" onchange="paging(this)" value="1" min="1" max="@Math.Ceiling(Model.Count() / 10d)"/>
            <span id="allPage">/ @Math.Ceiling(Model.Count() / 10d)</span>
        </span>
        <li class="inactive" title="Next page" onclick="paging(this)" id="nextPage">></li>
        <li class="inactive" title="Last page" onclick="paging(this)" id="lastPage">>></li>
    </ul>    
</div>